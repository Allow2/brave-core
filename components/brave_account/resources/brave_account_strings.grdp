  </message>
  <message name="IDS_BRAVE_ACCOUNT_CONFIRM_PASSWORD_INPUT_PLACEHOLDER" translateable="false" desc="Placeholder for the 'Confirm password' input field" formatter_data="webui=BraveAccount">
    Confirm your password
  </message>
  <message name="IDS_BRAVE_ACCOUNT_CONFIRM_PASSWORD_INPUT_ERROR_MESSAGE" translateable="false" desc="Message for a failed password confirmation" formatter_data="webui=BraveAccount">
    Passwords don't match
  </message>
  <message name="IDS_BRAVE_ACCOUNT_CONFIRM_PASSWORD_INPUT_SUCCESS_MESSAGE" translateable="false" desc="Message for a successful password confirmation" formatter_data="webui=BraveAccount">
    Passwords match
  </message>
  <message name="IDS_BRAVE_ACCOUNT_CREATE_ACCOUNT_BUTTON_LABEL" translateable="false" desc="Label for the button that submits the 'Create' dialog" formatter_data="webui=BraveAccount">
    Create account
  </message>

  <!-- 'Sign In' dialog: -->
  <message name="IDS_BRAVE_ACCOUNT_SIGN_IN_DIALOG_TITLE" translateable="false" desc="Title for the dialog where the user can sign in to their Brave Account" formatter_data="webui=BraveAccount">
    Sign in to your account
  </message>
  <message name="IDS_BRAVE_ACCOUNT_PASSWORD_INPUT_LABEL" translateable="false" desc="Label for the 'Password' input field" formatter_data="webui=BraveAccount">
    Password
  </message>
  <message name="IDS_BRAVE_ACCOUNT_FORGOT_PASSWORD_BUTTON_LABEL" translateable="false" desc="Label for the button that takes the user to the 'Forgot Password' dialog" formatter_data="webui=BraveAccount">
    Forgot your password?
  </message>
  <message name="IDS_BRAVE_ACCOUNT_SIGN_IN_BUTTON_LABEL" translateable="false" desc="Label for the button that submits the 'Sign In' dialog" formatter_data="webui=BraveAccount">
    Sign in
  </message>

  <!-- 'Forgot Password' dialog: -->
  <message name="IDS_BRAVE_ACCOUNT_FORGOT_PASSWORD_DIALOG_TITLE" translateable="false" desc="Title for the dialog where the user can reset their Brave Account password" formatter_data="webui=BraveAccount">
    Forgot your password?
  </message>
  <message name="IDS_BRAVE_ACCOUNT_FORGOT_PASSWORD_DIALOG_DESCRIPTION" translateable="false" desc="Description for the dialog where the user can reset their Brave Account password" formatter_data="webui=BraveAccount">
    Confirm your Brave account email and we'll email you a link to reset your password. The link will only be valid for 30 minutes.
  </message>
  <message name="IDS_BRAVE_ACCOUNT_ALERT_MESSAGE" translateable="false" desc="Alert message explaining that if the user resets their password without having an active login, all their data will be deleted" formatter_data="webui=BraveAccount">
    [Explain that if they reset their password without having an active login, all the user data will be deleted]
  </message>
  <message name="IDS_BRAVE_ACCOUNT_RESET_PASSWORD_BUTTON_LABEL" translateable="false" desc="Label for the button that submits the 'Forgot Password' dialog" formatter_data="webui=BraveAccount">
    Reset your password
  </message>

  <!-- 'Error' dialog: -->
  <message name="IDS_BRAVE_ACCOUNT_ERROR_DIALOG_TITLE" translateable="false" desc="Title for the error dialog" formatter_data="webui=BraveAccount">
    Something went wrong
  </message>
  <message name="IDS_BRAVE_ACCOUNT_ERROR_DIALOG_DESCRIPTION" translateable="false" desc="Description for the error dialog" formatter_data="webui=BraveAccount">
    We encountered an error while processing your request.
  </message>
  <message name="IDS_BRAVE_ACCOUNT_ERROR" translateable="false" desc="" formatter_data="webui=BraveAccount android_java">
    error
  </message>
  <message name="IDS_BRAVE_ACCOUNT_CLIENT_ERROR" translateable="false" desc="" formatter_data="webui=BraveAccount android_java">
    The browser encountered an unexpected error<ph name="ERROR_CODE">$1</ph>. Please restart the browser and try again.
  </message>
  <message name="IDS_BRAVE_ACCOUNT_SERVER_ERROR" translateable="false" desc="" formatter_data="webui=BraveAccount android_java">
    Internal server error (HTTP=<ph name="STATUS_CODE">$1</ph><ph name="ERROR_CODE">$2</ph>). Please try again later.
  </message>
  <message name="IDS_BRAVE_ACCOUNT_ERROR_DIALOG_INCORRECT_EMAIL" translateable="false" desc="" formatter_data="webui=BraveAccount">
    No account found for that email address. Please try again, or create a new account.
  </message>
  <message name="IDS_BRAVE_ACCOUNT_ERROR_DIALOG_INCORRECT_PASSWORD" translateable="false" desc="" formatter_data="webui=BraveAccount">
    Incorrect password. Please try again.
  </message>
  <message name="IDS_BRAVE_ACCOUNT_ERROR_DIALOG_ACCOUNT_EXISTS" translateable="false" desc="" formatter_data="webui=BraveAccount">
    An account already exists for this email address. Try logging in instead of creating an account.
  </message>
  <message name="IDS_BRAVE_ACCOUNT_ERROR_DIALOG_EMAIL_DOMAIN_NOT_SUPPORTED" translateable="false" desc="" formatter_data="webui=BraveAccount">
    The domain of this email address is not supported. Please use a different email address.
  </message>
  <message name="IDS_BRAVE_ACCOUNT_ERROR_DIALOG_TOO_MANY_VERIFICATIONS" translateable="false" desc="" formatter_data="webui=BraveAccount">
    Email verification limit exceeded. Please wait 30 minutes before trying again.
  </message>

  <!-- Common: -->
  <message name="IDS_BRAVE_ACCOUNT_DESCRIPTION" translateable="false" desc="Description explaining the benefits of having a Brave Account" formatter_data="webui=BraveAccount">
